{
  "name": "larva/laravel-kafka",
  "description": "This is a Laravel adapter for the Kafka.",
  "keywords": [ "kafka", "laravel" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-rdkafka": "*",
    "illuminate/database": "5.4.*|5.5.*||5.6.*|5.7.*|5.8.*",
    "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Kafka\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Kafka\\KafkaServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
